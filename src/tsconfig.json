{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2017", "es2018"],
    "module": "commonjs",
    "declaration": true,
    "sourceMap": false,
    "outDir": "lib",
    "strict": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "stripInternal": true,
    "importsNotUsedAsValues": "error",
    "paths": {
      "coc.nvim/*": ["../tests/coc.nvim/"]
    }
  },
  "include": ["."]
}
