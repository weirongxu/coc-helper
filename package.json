{
  "name": "coc-helper",
  "version": "0.6.0",
  "description": "Helpers for coc.nvim",
  "module": "lib/esm/index.js",
  "main": "lib/cjs/index.js",
  "homepage": "https://github.com/weirongxu/coc-helper",
  "repository": "git@github.com:weirongxu/coc-helper.git",
  "author": "Weirong Xu <weirongxu.raidou@gmail.com>",
  "license": "MIT",
  "files": [
    "lib",
    "tests",
    "JestHelper.js",
    "JestHelper.d.ts"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "copy:pkg": "cpy --rename=_package.json package.json src/",
    "build:esm": "tsc -p tsconfig.prod.json --module es2015 --outDir lib/esm",
    "build:cjs": "tsc -p tsconfig.prod.json --module commonjs --outDir lib/cjs",
    "build": "npm-run-all clean copy:pkg build:esm build:cjs",
    "lint": "eslint src --ext ts",
    "prepare": "npm-run-all clean build",
    "unittest": "jest --runInBand --detectOpenHandles --forceExit",
    "test": "npm-run-all copy:pkg lint unittest"
  },
  "engines": {
    "coc": "^0.0.77"
  },
  "activationEvents": [
    "*"
  ],
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "semi": true,
    "trailingComma": "all"
  },
  "peerDependencies": {
    "coc.nvim": "*"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/eslint": "^7.2.2",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.2",
    "@types/rimraf": "^3.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "coc.nvim": "^0.0.79",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.9.0",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "type-fest": "^0.16.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "app-root-path": "^3.0.0"
  }
}
