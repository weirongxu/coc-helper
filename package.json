{
  "name": "coc-helper",
  "version": "0.2.0",
  "description": "Helpers for coc.nvim",
  "main": "lib/src/index.js",
  "homepage": "https://github.com/weirongxu/coc-helper",
  "repository": "git@github.com:weirongxu/coc-helper.git",
  "author": "Weirong Xu <weirongxu.raidou@gmail.com>",
  "license": "MIT",
  "files": [
    "lib",
    "tests"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc -p tsconfig.prod.json",
    "lint": "eslint src --ext ts",
    "prepare": "npm-run-all clean build",
    "unittest": "jest --runInBand --detectOpenHandles --forceExit",
    "test": "npm-run-all lint unittest"
  },
  "engines": {
    "coc": "^0.0.77"
  },
  "activationEvents": [
    "*"
  ],
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "semi": true,
    "trailingComma": "all"
  },
  "peerDependencies": {
    "coc.nvim": "*"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/eslint": "^7.2.2",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/rimraf": "^3.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "coc.nvim": "^0.0.77",
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "type-fest": "^0.16.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "pkg-dir": "^4.2.0"
  }
}
